@using Syncfusion.JavaScript
@using Syncfusion.MVC.EJ

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>
        <link href="@Url.Content("~/favicon.ico")" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />    
        <meta name="description" content="Analyze the sales made by countries or by products, using map and range navigator controls | ASP.NET MVC">    
        <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/salesanalysis/Site.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/ejthemes/flat-azure-dark/ej.widgets.all.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/ejthemes/responsive-css/ej.responsive.css")" rel="stylesheet" type="text/css" />
        <!--[if lt IE 9]>
		<script src="@Url.Content("~/Scripts/jquery-1.11.3.min.js")"></script>
		<![endif]-->
		<!--[if gte IE 9]><!-->
		<script src="@Url.Content("~/Scripts/jquery-3.4.1.min.js")"></script>
		<!--<![endif]-->
        <script src="@Url.Content("~/Scripts/jquery.globalize.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jsrender.min.js")" type="text/javascript"></script>
       
        <script src="@Url.Content("~/Scripts/ej.web.all.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/SalesDashboard.js")" type="text/javascript"></script>
         <script src="@Url.Content("~/Scripts/WorldSalesData.js")" type="text/javascript"></script>
         	
    </head>
    <body>  
        @RenderBody()
       
        @(Html.EJ().ScriptManager())     
     @if (IsSectionDefined("ControlsSection"))
     {
          @RenderSection("ControlsSection", required: false)
     }
    
    </body>
</html>
