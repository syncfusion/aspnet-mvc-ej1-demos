@section SampleHeading{
	<span class="sampleName">PDF-Compress Existing PDF-ASP.NET MVC-SYNCFUSION</span>}
	
@section ControlsSection{


@{  using (Html.BeginForm("CompressExistingPDF", "Pdf", FormMethod.Post, new { enctype = "multipart/form-data"}))
    {
        <div class="Common">
            <div style = "font-size: 17px;font-weight: 400;">
			         This sample demonstrates the optimization of an existing PDF document based on image quality, page content, metadata, and font to reduce the file size of the PDF document.


	</div>
	<br/>
            <div class="rowdiv">
                <div class="celldiv">

                    <br />
                    <br />
                    <label>Choose a PDF file to compress</label>
                    <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                        <table width="100%">
                            <tr>
                                <td>
                                    <div>
                                        <input type="file" name="file" value="PDF document to compress" accept="application/pdf" />
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div id="errorlabel" value="error">
                        <p style="color: #FF0000;">
                            @Html.Label("", (string)ViewBag.lab)
                    </div>
                        <div class="alert alert-info" id="compressStatus" style="display:none">
                            The file compressed successfully.
                        </div>					
                    <br />
                    <br />
                    <label>
                        Compression Options
                    </label>
                    <br />
                    <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                        <table width="100%">
                            <tr>
                                <td align="left" width="50%" style="padding:5px 7px 5px 7px">
                                    <input type="checkbox" name="compressImage" value="true" checked="checked" onchange="isCompressImageOnChanged(this)" />
                                    <label for="Opentype">
                                        Compress Image
                                    </label>
                                </td>
                                <td align="right" width="50%" style="padding:5px 7px 5px 7px">
                                    <label for="Opentype">
                                        Image Quality &nbsp;&nbsp;&nbsp;&nbsp;
                                    </label>
                                    @Html.DropDownList("imageQuality") &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                                </td>
                            </tr>
                            <tr>
                                <td align="left" width="50%" style="padding:5px 7px 5px 7px">
                                    <input type="checkbox" name="optPageContents" value="true" checked="checked" />
                                    <label for="Opentype">
                                        Optimize Page Contents
                                    </label>
                                </td>
                                <td align="right" width="50%" style="padding:5px 7px 5px 7px">
                                    <input type="checkbox" name="removeMetaData" value="true" checked="checked" />
                                    <label for="Opentype">
                                        Remove Metadata Information
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td align="left" width="50%" style="padding:5px 7px 5px 7px">
                                    <input type="checkbox" name="optFont" value="true" checked="checked" />
                                    <label for="Opentype">
                                        Optimize Font
                                    </label>
                                </td>
                                <td align="right" width="50%" style="padding:5px 7px 5px 7px">
                                    <label for="Opentype">
                                    </label>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <br />
            <br />
            <div class="celldiv">
                <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                    <table width="100%">
                        <tr>
                            <td align="left" width="100%">
                                <input id="Checkbox1" type="checkbox" name="Browser" value="Browser" />
                                <label for="Opentype">
                                    Open Document inside Browser
                                </label>
                            </td>
                            <td align="right">
                                <label>
                                    <input id="submit" type="submit" value="Compress PDF" />
                                </label>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
			<br/>
			<div style = "font-size: 17px;font-weight: 400;">
		
					
		This feature can be used to reduce response times in web based applications, mails and more.
More information about compression can be found in this documentation <a href="https://help.syncfusion.com/file-formats/pdf/working-with-compression#compressing-existing-pdf-document">section.</a>
           </div>
        </div>
        Html.EndForm();
    }}}
<script type="text/javascript">
    function isCompressImageOnChanged(element) {
        element.checked ? document.getElementById("imageQuality").disabled = false : document.getElementById("imageQuality").disabled = true;
    }
</script>
<script type="text/javascript">
    (function worker() {
        $.ajax({
            url: '@Url.Action("GetStatus", "Pdf")',
            success: function (data) {

                if (data != "") {
                    document.getElementById("compressStatus").innerHTML = data;
                    document.getElementById('compressStatus').removeAttribute("style");

                }


            },
            complete: function () {
                setTimeout(worker, 300);
            }
        });
    })();
</script>
@section MetaTags{
   <meta name="description" content="Example [C#] to compress the PDF document in ASP.NET MVC using Syncfusion .NET PDF library.">
}

