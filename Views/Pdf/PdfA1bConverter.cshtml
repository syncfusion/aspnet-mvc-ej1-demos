

@section SampleHeading{
	<span class="sampleName">PDF-PDF to PDFA1b-ASP.NET MVC-SYNCFUSION</span>}
	

    @section ControlsSection{
     @{Html.BeginForm("PdfA1bConverter", "Pdf", FormMethod.Post, new { enctype = "multipart/form-data" });
         {
    <div class="Common">
       <div style = "font-size: 17px;font-weight: 400;">
			         This sample demonstrates the conversion of PDF to PDF/A-1b standard document using Essential PDF.

	</div>
	<br/>
        <div class="tablediv">            
            <div class="rowdiv">
                

                    <div class="celldiv">
                    <label>Choose a PDF file to convert</label>
                    <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                        <table width="100%">
                            <tr>
                                <td>
                                    <div>
                                        <input type="file" name="file" value="PDF document to convert" accept=".pdf" />
                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div id="errorlabel" value="error">
                        <p style="color: #FF0000;">
                            @Html.Label("", (string)ViewBag.lab)
                    </div>
                     <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                        <table width="100%">
                            <tr>
                                <td align="left" width="100%">
                                    <input id="Checkbox1" type="checkbox" name="Browser" value="Browser" />
                                    <label  for="Opentype">Open Document inside Browser</label>
                                </td>
                                <td align="right">
                                    <label><input id="submit" type="submit" value="Convert PDF" /></label>
                                </td>
                            </tr>
                        </table>
                      </div>
                        
                        
                    </div>
                </div> 
            </div>   
<br/>
			<div style = "font-size: 17px;font-weight: 400;">
		
			This feature can be used for long term archiving and standardization.
More information about conformance can be found in this documentation <a href="https://help.syncfusion.com/file-formats/pdf/working-with-pdf-conformance">section.</a>
           </div>			
    </div>
    Html.EndForm();
      }}}
@section MetaTags{
   <meta name="description" content="Example [C#] to convert PDF to PDF/A-1b in ASP.NET MVC using Syncfusion .NET PDF library.">
}


