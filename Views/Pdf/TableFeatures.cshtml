

@section SampleHeading{
	<span class="sampleName">PDF-Table Features-ASP.NET MVC-SYNCFUSION</span>}
	


    @section ControlsSection{
    @{Html.BeginForm("TableFeatures", "Pdf", FormMethod.Post);
      { 
    <div class="Common">
        <div style = "font-size: 17px;font-weight: 400;">
			       This sample demonstrates the creation of richly formatted table to keep your data organized for both basic and advanced operation on cells, rows, and columns along with headers and footers.

	</div>
	<br/>
        <div class="tablediv">
        <div class="rowdiv">
            <div class="celldiv">
             <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
             <table width="100%">
                        <tr>
                            <td align="left" width="100%">
                                <input id="InsideBrowser" type="checkbox" name="InsideBrowser" value="Browser" />
                <label for="Opentype">
                    Open Document inside Browser</label> 
                            </td>
                            <td align="right">
                                  <input type="submit" class="Button" value="Create PDF" />
                            </td>
                        </tr>
                    </table>
               </div>
                
                
            </div>
        </div>
        </div>
		<br/>
			<div style = "font-size: 17px;font-weight: 400;">
		
				
		More information about the document settings can be found in this documentation <a href="https://help.syncfusion.com/file-formats/pdf/working-with-tables"> section. </a>

           </div>
    </div>
    Html.EndForm();
      }}}
@section MetaTags{
   <meta name="description" content="Example [C#] to create richly formatted table with defined styles in ASP.NET MVC using Syncfusion .NET PDF library.">
}


