@section SampleHeading{
	<span class="sampleName">Getting Started / PDF Compression Sample</span>
	}
@section ControlsSection{


@{Html.BeginForm("PDFCompression", "Pdf", FormMethod.Post);
  { 
    <div class="Common">
        <div style = "font-size: 17px;font-weight: 400;">
			       This sample demonstrates the creation of compressed PDF document. It compresses the content stream and images in the PDF document based on the quality factor.

	</div>
	<br/>
        <div class="rowdiv">
            <div class="celldiv">
                <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                    <table width="100%">
                        <tr>
                            <td align="left" width="80%">
                                <label>
                                    Choose the compression level</label>
                                <br />
                                @Html.DropDownList("compress")
                            </td>
                            <td align="right">
                                <label>
                                    Image Compression</label><br />@Html.DropDownList("img")
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div class="celldiv">
            <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
                <table width="100%">
                    <tr>
                        <td align="left" width="100%">
                            <input id="Checkbox1" type="checkbox" name="Browser" value="Browser" />
                            <label for="Opentype">
                                Open Document inside Browser</label>
                        </td>
                        <td align="right">
                            <label>
                                <input id="submit" type="submit" value="Create PDF" /></label>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
		<br/>
			<div style = "font-size: 17px;font-weight: 400;">
		
					
		It is also possible to  <a href="https://help.syncfusion.com/file-formats/pdf/working-with-compression#compressing-existing-pdf-document">compress the existing PDF document</a> by removing the metadata, optimizing the font, and page content.
           </div>
    </div>
        
        
        
    
      Html.EndForm();
  }}}
