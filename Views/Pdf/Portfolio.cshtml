@section SampleHeading{
	<span class="sampleName">PDF-Portfolio-ASP.NET MVC-SYNCFUSION</span>}
	


@section ControlsSection{
    @{Html.BeginForm("Portfolio", "Pdf", FormMethod.Post);
      { 
    <div class="Common">
        <div class="tablediv">
            <div style = "font-size: 17px;font-weight: 400;">
			       This sample demonstrates how to create portfolio in the PDF document. You can also extract and remove files from the PDF portfolio.

<br/>
This feature allows the user to bring content together from variety of sources including documents, drawings, images, emails, spreadsheets, and web pages.


	</div>
	<br/>
        
        <div class="rowdiv">
            <div class="celldiv">
             <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
             <table width="100%">
                        <tr>
                            <td align="left" width="100%">
                                 <input id="InsideBrowser" type="checkbox" name="InsideBrowser" value="Browser" />
                                    <label for="Opentype">
                                        Open Document inside Browser</label>
                            </td>
                            <td align="right">
                                   <input type="submit" class="Button" value="Create PDF" />
                            </td>
                        </tr>
                    </table>
               </div>
                
               
            </div>
        </div>
        </div>
		<br/>
			<div style = "font-size: 17px;font-weight: 400;">
More information about the portfolio can be found in this documentation <a href="https://help.syncfusion.com/file-formats/pdf/working-with-portfolio">section.</a>

           </div>
    </div>
    Html.EndForm();
      }}}
@section MetaTags{
   <meta name="description" content="Example [C#] to create PDF portfolio in ASP.NET MVC using Syncfusion .NET PDF library.">
}

