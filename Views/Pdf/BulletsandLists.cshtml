
@section SampleHeading{
	<span class="sampleName">PDF-Bullets and Lists-ASP.NET MVC-SYNCFUSION</span>}
	


    @section ControlsSection{
    @{Html.BeginForm("BulletsLists", "Pdf", FormMethod.Post);
      { 
    <div class="Common">
        <div style = "font-size: 17px;font-weight: 400;">
			       This sample demonstrates how to list the content in ordered and unordered list. The ordered list can be number or alphabets and the unordered list can be bullets, circles, and images.

	</div>
	<br/>
        <div class="tablediv">
        <div class="rowdiv">
            <div class="celldiv">
             <div style="border: solid 1px #EDEDED; padding: 5px 7px 5px 7px;">
             <table width="100%">
                        <tr>
                            <td align="left" width="100%">
                                <input id="InsideBrowser" type="checkbox" name="InsideBrowser" value="Browser" />
                <label for="Opentype">
                    Open Document inside Browser</label>
                            </td>
                            <td align="right">
                                  <input type="submit" class="Button" value="Create PDF" />
                            </td>
                        </tr>
                    </table>
               </div>
                
                
            </div>
        </div>
        </div>
		<br/>
    </div>
    Html.EndForm();
      }}}
@section MetaTags{
   <meta name="description" content="Example [C#] to create ordered and unordered list in PDF document in ASP.NET MVC using Syncfusion .NET PDF library.">
}


