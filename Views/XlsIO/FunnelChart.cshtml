@using Syncfusion.JavaScript.DataVisualization


@section SampleHeading{<span class="sampleName">XlsIO-Funnel-ASP.NET MVC-SYNCFUSION</span>}

 @section ControlsSection{                                
	      @{
              ViewBag.Title = "    Essential XlsIO (Excel) : Funnel Chart : Syncfusion";}

@{Html.BeginForm("FunnelChart", "XlsIO", FormMethod.Post);
{
    <div class="Common">
        <div class="tablediv">
            <div class="rowdiv">
                Funnel charts are a type of chart, often used to represent stages in a sales process and show the amount of potential revenue for each stage. This type of chart can also be useful in identifying potential problem areas in an organization's sales processes.<br />
                <br />Essential XlsIO has APIs for creating and modifying funnel charts.
            </div>
            <br />
            <div class="rowdiv">
                <label>
                    Click the button to view an Excel spreadsheet generated by Essential XlsIO. Please
                    note that Microsoft Excel Viewer or Microsoft Excel is required to view the resultant document.
                </label>
            </div>
            <br />
            <div class="rowdiv">
                <div class="celldiv">
                    <label>
                        <strong> Save As :</strong>
                    </label>
                    <input id="rbtnEmbeddedChart" type="radio" name="SaveOption" value="embed" checked="checked" />
                    <label for="rbtnEmbeddedChart">
                        Embedded Chart
                    </label>
                    &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                    <input id="rbtnChartSheet" type="radio" name="SaveOption" value="sheet" />
                    <label for="rbtnChartSheet">
                        Chart Sheet
                    </label>
                </div>
                <br />
                <div class="celldiv">
                    <input class="buttonStyle" type="submit" name="button" value="Create Document" style=" width:200px;" />
                    <input class="buttonStyle" type="submit" name="button" value="Input Template" style=" width:200px;" />
                </div>
            </div>
        </div>
    </div>
    Html.EndForm();
} }

               
              }

