@using Syncfusion.JavaScript.DataVisualization


@section SampleHeading{<span class="sampleName">XlsIO / Import JSON</span>}

 @section ControlsSection{                                
	     @{
    ViewBag.Title = " Essential XlsIO (Excel) : Import JSON : Syncfusion";
}

@{Html.BeginForm("ImportJSON", "XlsIO", FormMethod.Post);
{
    <div class="Common">
        <div class="tablediv">
            <div class="rowdiv">
                Essential XlsIO allows you to import dynamic objects into the Excel workbook.The dynamic objects can be either the ExpandoObjects or user-defined dynamic objects.
                <br />
                <br />
                In this sample, user-defined dynamic objects have been created at run time from JSON file and imported into Excel workbook.
            </div>
            <br />
            <div class="rowdiv">
                <label>
                    Click the button to view an Excel spreadsheet generated by Essential XlsIO. Please
                    note that Microsoft Excel Viewer or Microsoft Excel is required to view the resultant document.
                </label>
            </div>
            <div class="rowdiv">                
                <br />
                <div class="celldiv">
                    <label>
                        <strong>  Save As :</strong>
                    </label>
                    <input id="rdbXls" type="radio" name="SaveOption" value="Xls" />
                    <label for="rdbXls">
                        XLS
                    </label>
                    &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                    <input id="rdbXlsx" type="radio" name="SaveOption" value="Xlsx" checked="checked" />
                    <label for="rdbXlsx">
                        XLSX
                    </label>                                       
                </div>
                <br />
                <div class="celldiv">
                    <input class="buttonStyle" type="submit" value="Create Document" style="width:200px;" />
                </div>
            </div>
        </div>
    </div>
    Html.EndForm();
} }
         
}
