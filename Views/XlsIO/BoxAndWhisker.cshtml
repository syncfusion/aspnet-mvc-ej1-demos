@using Syncfusion.JavaScript.DataVisualization


@section SampleHeading{<span class="sampleName">XlsIO-Box and Whisker-ASP.NET MVC-SYNCFUSION</span>}

 @section ControlsSection{                                
	      @{
              ViewBag.Title = "    Essential XlsIO (Excel) : Box And Whisker : Syncfusion";}

@{Html.BeginForm("BoxAndWhisker", "XlsIO", FormMethod.Post);
{
    <div class="Common">
        <div class="tablediv">
            <div class="rowdiv">
                A box and whisker chart shows distribution of data into quartiles, highlighting the mean and outliers. The boxes may have lines extending vertically called “whiskers”. These lines indicate variability outside the upper and lower quartiles, and any point outside those lines or whiskers is considered an outlier.<br />
                <br />Essential XlsIO has APIs for creating and modifying Box and Whisker charts.
            </div>
            <br />
            <div class="rowdiv">
                <label>
                    Click the button to view an Excel spreadsheet generated by Essential XlsIO. Please
                    note that Microsoft Excel Viewer or Microsoft Excel is required to view the resultant document.
                </label>
            </div>
            <br />
            <div class="rowdiv">
                <div class="celldiv">
                    <label>
                        <strong> Save As :</strong>
                    </label>
                    <input id="rbtnEmbeddedChart" type="radio" name="SaveOption" value="embed" checked="checked" />
                    <label for="rbtnEmbeddedChart">
                        Embedded Chart
                    </label>
                    &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                    <input id="rbtnChartSheet" type="radio" name="SaveOption" value="sheet" />
                    <label for="rbtnChartSheet">
                        Chart Sheet
                    </label>
                </div>
                <br />
                <div class="celldiv">
                    <input class="buttonStyle" type="submit" name="button" value="Create Document" style=" width:200px;" />
                    <input class="buttonStyle" type="submit" name="button" value="Input Template" style=" width:200px;" />
                </div>
            </div>
        </div>
    </div>
    Html.EndForm();
} }

               
              }

