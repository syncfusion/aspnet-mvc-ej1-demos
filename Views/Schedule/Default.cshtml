
@section MetaTags{
    <meta name="description" content="This example shows how the ASP.NET MVC Scheduler looks with its default configuration.">
}
@section SampleHeading{<span class="sampleName">Schedule-Default Functionalities-ASP.NET MVC-SYNCFUSION</span>}
@section ControlsSection{
<div>
        @(Html.EJ().Schedule("Schedule1")
        .Width("100%")
        .Height("525px")
        .CurrentDate(new DateTime(2017,6,5))
        .AppointmentSettings(fields => fields.Datasource((IEnumerable)ViewBag.datasource)
            .Id("Id")
            .Subject("Subject")
            .StartTime("StartTime")
            .EndTime("EndTime")
            .Description("Description")
            .AllDay("AllDay")
            .Recurrence("Recurrence")
            .RecurrenceRule("RecurrenceRule"))
        )
</div>
}

