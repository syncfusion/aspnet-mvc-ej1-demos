@section SampleHeading{
    <span class="sampleName">Autocomplete-RTL-ASP.NET MVC-SYNCFUSION</span>}
@section MetaTags{
    <meta name="description" content="This example demonstrates how the suggestion list content is displayed from right to left in the Syncfusion ASP.NET MVC Autocomplete component.">
}
@section ControlsSection{
    <div class="frame">
        <div class="control">
            @{
                Html.EJ()
                    .Autocomplete("selectCar")
                    .Width("100%")
                    .Datasource((IEnumerable<CarsList>)ViewBag.datasource)
                    .EnableRTL(true)
                    .WatermarkText("Select a car")
                    .Render();
            }
        </div>
    </div>
}

